package alg.DP;
import java.util.*;

public class dp_06_11052 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] p = new int[n];
		int[] d = new int[1001];

		for(int i = 0; i < n; i++) {
			p = sc.nextInt();
		}

		d[1] = p[0];

		for(int i = 2; i <= n; i++) {
			if(p[i-1] * i > p[i]) {
				d[i] = p[i-1] * i;
			} else {
				p[i];
			}
		}
	}
}